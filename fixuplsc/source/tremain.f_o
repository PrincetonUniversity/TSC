      subroutine tremain(secleft)
c
c.....returns the number of seconds left before
c.....program times out
!============
! idecl:  explicitize implicit REAL declarations:
      IMPLICIT NONE
      INTEGER, PARAMETER :: R8=SELECTED_REAL_KIND(12,100)
      REAL*8 secleft
!============
      secleft = 1000._R8
      return
      end
